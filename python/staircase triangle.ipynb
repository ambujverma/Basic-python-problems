    "def staircase():\n",
    "    n =int(input('how many staircase you want to print'))\n",
    "    \n",
    "    n1=n-1\n",
    "    i=1\n",
    "    for i in range(1,n):\n",
    "         print(' '*(n1-i),'#' ' '*i)\n",
    "            \n",
    "    print('#' ' '*n)\n",
    "    \n",
    "\n",
    "    \n",
    "    \n",
    "staircase()\n",
    "        \n"
